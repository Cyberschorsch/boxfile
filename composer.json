{
    "name": "derhasi/boxfile",
    "description": "Command to handle freistil Boxfiles anywhere",
    "repositories": [
	{
            "type": "vcs",
            "url": "https://github.com/Cyberschorsch/symlinker.git"
        }
    ],
    "require": {
        "symfony/yaml": "2.*",
        "symfony/console": "2.*",
        "symfony/config": "~2.6",
        "derhasi/symlinker": "dev-update-dependencies"
    },
    "type": "cli",
    "license": "GPLv2+",
    "authors": [
        {
            "name": "Johannes Haseitl",
            "email": "johannes@undpaul.de"
        }
    ],
    "autoload": {
      "psr-4": {
        "derhasi\\boxfile\\": "src/"
      }
    },
    "bin": [
      "boxfile"
    ]
}
